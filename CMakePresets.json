{
  "version": 3,
  "configurePresets": [
    {
      "name": "windows-debug",
      "binaryDir": "build",
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "cl.exe",
        "CMAKE_CXX_COMPILER": "cl.exe",
        "CMAKE_C_LINK_EXECUTABLE": "link.exe",
        "CMAKE_CXX_LINK_EXECUTABLE": "link.exe",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
      }
    }
  ]
}
